# see CMakeLists.txt for details

PROJECT_NAME           = "ci_example_cpp"
OUTPUT_DIRECTORY       = doc
JAVADOC_AUTOBRIEF      = YES
TAB_SIZE               = 4
EXTRACT_ALL            = NO
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
INPUT                  = @PROJECT_SOURCE_DIR@/include/ci_example @PROJECT_SOURCE_DIR@/demos @PROJECT_SOURCE_DIR@/tests @PROJECT_SOURCE_DIR@/src/demos
FILE_PATTERNS          = *.h \
                         *.hpp \
			 *.cpp \
RECURSIVE              = YES
GENERATE_LATEX         = NO
GENERATE_TAGFILE       = @PROJECT_NAME@.tag